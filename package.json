{
	"name": "@iot-ensemble/ensemble",
	"version": "0.0.1",
	"scripts": {
		"build:common": "ng build common",
		"build:devices": "ng build devices --prod",
		"build:public": "ng build public --prod",
		"deploy": "npm version patch && npm run deploy:all",
		"deploy:all": "npm run deploy:common && npm run deploy:public && npm run deploy:devices",
		"deploy:common": "npm version patch --prefix projects/common && npm run build:common && npm publish ./dist/common --access public",
		"deploy:devices": "npm version patch --prefix projects/devices/ && npm run build:devices && npm publish ./dist/devices --access public",
		"deploy:public": "npm version patch --prefix projects/public/ && npm run build:public && npm publish ./dist/public --access public",
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e"
	},
	"private": false,
	"dependencies": {
		"@angular/animations": "~7.2.0",
		"@angular/cdk": "^7.3.6",
		"@angular/common": "~7.2.0",
		"@angular/compiler": "~7.2.0",
		"@angular/core": "~7.2.0",
		"@angular/flex-layout": "^7.0.0-beta.24",
		"@angular/forms": "~7.2.0",
		"@angular/material": "^7.3.6",
		"@angular/platform-browser": "~7.2.0",
		"@angular/platform-browser-dynamic": "~7.2.0",
		"@angular/router": "~7.2.0",
		"@aspnet/signalr": "^1.1.2",
		"@lcu-ide/common": "^0.1.27-integration",
		"@lowcodeunit-devkit/ng-lcu": "^0.1.20-integration",
		"core-js": "^2.5.4",
		"hammerjs": "^2.0.8",
		"rxjs": "~6.3.3",
		"tslib": "^1.9.0",
		"zone.js": "~0.8.26"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~15.0.4",
		"@angular-devkit/build-ng-packagr": "~0.13.0",
		"@angular/cli": "~7.3.4",
		"@angular/compiler-cli": "~7.2.0",
		"@angular/language-service": "~7.2.0",
		"@lcu/cli": "^0.1.4-integration",
		"@types/jasmine": "~2.8.8",
		"@types/jasminewd2": "~2.0.3",
		"@types/node": "~8.9.4",
		"codelyzer": "~4.5.0",
		"jasmine-core": "~2.99.1",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "~4.0.0",
		"karma-chrome-launcher": "~2.2.0",
		"karma-coverage-istanbul-reporter": "~2.0.1",
		"karma-jasmine": "~1.1.2",
		"karma-jasmine-html-reporter": "^0.2.2",
		"ng-packagr": "^4.2.0",
		"protractor": "~5.4.0",
		"ts-node": "~7.0.0",
		"tsickle": ">=0.34.0",
		"tslib": "^1.9.0",
		"tslint": "~5.11.0",
		"typescript": "~3.2.2"
	}
}
